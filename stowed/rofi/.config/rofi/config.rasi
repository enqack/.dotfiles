configuration {
  show-icons: true;
  monitor: "primary";
  font: "DejaVuSansMono Nerd Font 18";
  modi: "window,run,drun,ssh,filebrowser";

  filebrowser {
    directories-first: true;
  }

  display-drun: "Select Application From XDG";
  display-run: "Select Application From $PATH";
}

@theme "bspwm-theme"

